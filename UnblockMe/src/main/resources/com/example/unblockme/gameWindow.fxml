<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="882.0" prefWidth="1259.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.unblockme.GameWindowController">
   <children>
      <TabPane fx:id="TabPaneMy" prefHeight="882.0" prefWidth="1259.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="GameTab" text="Game">
            <content>
              <AnchorPane fx:id="Gamestage" minHeight="0.0" minWidth="0.0" prefHeight="666.0" prefWidth="893.0">
                     <children>
                        <Canvas fx:id="canvas" height="856.0" layoutY="-1.0" width="1259.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-1.0" />
                        <Button layoutX="766.0" layoutY="636.0" mnemonicParsing="false" onAction="#ExitFromLevel" prefHeight="64.0" prefWidth="137.0" text="Exit">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Button>
                        <Button layoutX="766.0" layoutY="556.0" mnemonicParsing="false" onAction="#RestartLevel" prefHeight="64.0" prefWidth="137.0" text="Restart" textAlignment="CENTER">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="HelpTab" text="Help">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="724.0" fitWidth="957.0" layoutY="-1.0">
                           <image>
                              <Image url="@../../../NFS_about.jpg" />
                           </image>
                        </ImageView>
                        <Button layoutX="43.0" layoutY="556.0" mnemonicParsing="false" onAction="#CreateWindowWithRules" prefHeight="126.0" prefWidth="325.0" text="Rules">
                           <font>
                              <Font size="38.0" />
                           </font>
                        </Button>
                        <Button layoutX="601.0" layoutY="556.0" mnemonicParsing="false" onAction="#CreateWindowWithLeaderBoard" prefHeight="126.0" prefWidth="325.0" text="LeaderBoard">
                           <font>
                              <Font size="38.0" />
                           </font>
                        </Button>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
